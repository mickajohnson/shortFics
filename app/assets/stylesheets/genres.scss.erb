@font-face {
  font-family: "Blackfat_Demo";
  src: font-url('/assets/Blackfat_Demo.ttf') format("truetype");
  font-weight: normal;
  font-style: normal;
}
@font-face {
  font-family: "SpaceMono-Regular";
  src: font-url('/assets/SpaceMono-Regular.ttf') format("truetype");
  font-weight: normal;
  font-style: normal;
}

p {
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  margin-bottom: 15px;
  font-size: 1.3em;
  font-weight: 200;
}

h1, h2, h3, h4, h5, h6 {
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  font-weight: 200;
}

.big-header {
  margin-top: 0px;
}

.blackfat {
  font-family: "Blackfat_Demo";
}

.well{
  margin-bottom: 0px;
  text-align: center;
}
.jumbotron{
  text-align: center;
}
.carousel-control.left, .carousel-control.right {
   background-image:none !important;
   filter:none !important;
   color: #262248;
}

.carousel-content {
  color:black;
  display:flex;
  align-items:center;
}

footer{
  background: #F8F8F8;
  height: 60px;  /* footer height */
  display: block;
  box-sizing: border-box;
  position: relative;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  p{
    margin-bottom: 0px;
  }
}
.navbar-toggle{
  margin-top: 15px;
}
.main{
  min-height: calc(100vh - 152px);
}


.inline{
  display: inline-block;
}
.form-control {
  margin-bottom: 1em;
}

.jumbotron {
  background-color: #FFFFFF;
  border: 0px;
}
.well {
  background-color: #FFFFFF;
  padding-top: 0px
}

.footer {
  background-color: #FFFFFF;
}

#nav-button {
  border: 0px solid #FFFFFF;
  padding: 0px;
}

.hover-undeline:hover {
  text-decoration: underline;
}

#no-border {
  border: 0px solid #FFFFFF;
  box-shadow: 0px 0px 0px #FFFFFF;
}

.black-button {
  background-color: #262248
}
.navy-button {
  background-color: #262262
}
.orange-button {
  background-color: #DFAB72
}
.black-button:hover {
  color: #DFAB72
}
.navy-button:hover {
  color: #DFAB72
}
.orange-button:hover {
  color: #262262
}

.navbar-brand {
  padding: 0px;
  display: inline-block;
  vertical-align: center;
}
.navbar-brand>img {
  height: 100%;
  padding: 0px;
  display: inline-block;
  vertical-align: center;
  width: auto;
}

#big-logo {
  margin-left: -15px;
}
.login {
  margin-top: -50px
}

.author {
  margin: 10px
}

.brand-logo {
  position: absolute;
  margin-top:5px;
  left: 47.5%;
  padding: 0px;
  width: 65px;
}

.dropdown-toggle {
  padding-top: 0px
}

.navbar {
  margin-bottom: 0px
}

.btn {
  color: white;
}

.navbar-default .navbar-nav > li > a:hover, .navbar-default .navbar-nav > li > a:focus {
color: #000;  /*Sets the text hover color on navbar*/
}

.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active >
 a:hover, .navbar-default .navbar-nav > .active > a:focus {
color: white; /*BACKGROUND color for active*/
background-color:#FFFFFF;
}

  .navbar-default {
    background-color: #FFFFFF;
    border-color: #FFFFFF;
}

  .dropdown-menu > li > a:hover,
   .dropdown-menu > li > a:focus {
    color: #262626;
   text-decoration: none;
  background-color: #DFAB72;  /*change color of links in drop down here*/
   }

 .nav > li > a:hover,
 .nav > li > a:focus {
    text-decoration: none;
    background-color: #000000; /*Change rollover cell color here*/
  }

  .navbar-default .navbar-nav > .open > a, .navbar-default .navbar-nav > .open > a:hover, .navbar-default .navbar-nav > .open > a:focus {
    background-color: #FFFFFF
  }




  .navbar-default .navbar-nav > li > a {
   color: #262248; /*Change active text color here*/
    }
.read_more {
  display: inline-block;
}


@media screen and (max-width: 462px){
  .btn{
    display: block;
    margin-bottom: 2px;
  }
}

@media screen and (max-width: 774px){
  .nav li{
    margin-top: 0px;
  }
}
.nav li{
  margin-top: 10px;
}
.navbar-nav{
  margin-top: -5px;
}
